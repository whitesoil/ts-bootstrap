{
    "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
    "rules": {
        "no-trailing-whitespace": [true],
        "trailing-comma":false,
        "import-name": [false],
        "semicolon": [true, "always"],
        "indent": [true, "spaces"],
        "max-line-length": [true, 120]
    }
}